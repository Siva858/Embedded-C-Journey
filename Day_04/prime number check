#include <stdio.h>
#include <math.h>

int main()
{
    int n,count=0;
    printf("enter the number u want to check ");
    scanf("%d",&n);
    
    if(n<=1){
        printf("it is not prime number");
    }
    
    for(int i=2;i<=sqrt(n);i++){
        if(n%i==0){
            count++;
            break;
        }
    }
    if(count==0){
        printf("yes it is %d is prime number.\n",n);
    }else{
        printf("%d is not prime.\n",n);
    }
    

    return 0;
}
